{
  "agent": {
    "default_model": {
      "provider": "zed.dev",
      "model": "claude-3-5-sonnet-latest"
    },
    "model_parameters": [],
    "default_profile": "ask",
    "version": "2"
  },
  "features": {
    "edit_prediction_provider": "zed"
  },
  "icon_theme": "Material Icon Theme",
  "base_keymap": "VSCode",
  "languages": {
    "Vue.js": {
      "formatter": {
        "code_actions": {
          "source.fixAll.eslint": true
        }
      }
    },
    "TypeScript": {
      "formatter": {
        "code_actions": {
          "source.fixAll.eslint": true
        }
      }
    },
    "JavaScript": {
      "formatter": {
        "code_actions": {
          "source.fixAll.eslint": true
        }
      }
    },
    "JSON": {
      "formatter": {
        "code_actions": {
          "source.fixAll.eslint": true
        }
      }
    },
    "CSS": {
      "format_on_save": "off"
    }
  },
  "ui_font_size": 16,
  "buffer_font_size": 18,
  "buffer_line_height": { "custom": 1.85 },
  "buffer_font_family": "Geist Mono",
  "buffer_font_weight": 500,
  "buffer_font_features": {
    "liga": false
  },
  "theme": {
    "mode": "system",
    "light": "One Light",
    "dark": "One Dark"
  },
  "terminal": {
    "option_as_meta": false
  }
}
